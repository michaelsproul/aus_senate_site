<table class="pure-table pure-table-horizontal">
<thead>
  <tr>
    <th>#</th>
    <th>Name</th>
    <th>Party</th>
    <th>Vote Count</th>
  </tr>
</thead>
<tbody>
{{#each elected}}
  <tr>
    <td>{{this.position}}</td>
    <td>{{this.candidate.other_names}} {{this.candidate.surname}}</td>
    <td>{{this.candidate.party}}</td>
    <td>{{this.vote_tally}}</td>
  </tr>
{{/each}}
</tbody>
</table>
{{#if disqualified}}
<p>The following senators were excluded from the calculation, and had their votes transferred
appropriately.</p>
<ul>
{{#each disqualified}}
  <li>{{this.first}} {{this.last}}</li>
{{/each}}
</ul>
{{/if}}
